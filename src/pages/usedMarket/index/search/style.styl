.back{
    width:100%;
    height:100vh;
    background-color: #eeeeee;


.record{
    width:100%;
    font-size: 30rpx;
    padding:0 32rpx;
    background-color: white;
    margin:20rpx auto;
    .record_text{
        padding:20rpx 0 10rpx 0;
        p{
            display: inline-block;
            font-weight: 400;
        }
        .img_fr{
            width:31rpx;
            height:31rpx;
            float:right;}
        }
    .record_tip{
        display: inline-block;
        padding-bottom: 20rpx;
        .tip{
            padding:10rpx 17rpx;
            margin:10rpx;
            border-radius: 13px;
            background-color: #f1f1f1;
            color:gray;}
        }
}
}